<h2 >Productos agregados</h2>

<% if(items.length > 0) { %>
  <div class="table-responsive">
    <table class="table table-dark">
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Imagen</th>
      </tr>
      <% for (const item of items) {%>
      <tr>
        <td><%=item.name%></td>
        <td><%=item.price%></td>
        <td><img src="<%=item.thumbnail%>" alt="${item.name}"></td>
      </tr>
      <%};%>
    </table>
  </div>
<%}else{%>
  <br>
  <h3 class="alert alert-danger">No se encontraron datos</h3>
<%};%>

